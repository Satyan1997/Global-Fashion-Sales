        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>

        public void Main()
        {
            try
            {
                string smtpServer = Dts.Variables["$Project::SmtpServer"].Value.ToString();
                int smtpPort = Convert.ToInt32(Dts.Variables["$Project::SmtpPort"].Value);
                string smtpUser = Dts.Variables["$Project::SmtpUsername"].Value.ToString();
                string smtpPass = Dts.Variables["$Project::SmtpPassword"].Value.ToString();
                string fromEmail = Dts.Variables["$Project::FromEmail"].Value.ToString();
                string toEmail = Dts.Variables["$Project::ToEmail"].Value.ToString();
                string subject = Dts.Variables["User::EmailSubjectFileNotExists"].Value.ToString();
                string body = Dts.Variables["User::EmailBodyFileNotExists"].Value.ToString();

                MailMessage mail = new MailMessage();
                mail.From = new MailAddress(fromEmail);

                foreach (string address in toEmail.Split(new char[] { ';' }, StringSplitOptions.RemoveEmptyEntries))
                {
                    mail.To.Add(address.Trim());
                }

                mail.Subject = subject;
                mail.Body = body;

                SmtpClient client = new SmtpClient(smtpServer, smtpPort)
                {
                    EnableSsl = true,
                    Credentials = new NetworkCredential(smtpUser, smtpPass)
                };

                client.Send(mail);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "File Not Found Email", ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

    }

