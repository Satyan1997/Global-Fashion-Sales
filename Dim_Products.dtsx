        public void Main()
        {
            // TODO: Add your code here

            try
            {
                string folder = Dts.Variables["$Project::SourceFolderPath"].Value.ToString();

                Dts.Variables["User::GetDate"].Value = DateTime.Now;

                bool fileFound = false;
                // Check if any files matching the pattern exist in the folder
                if (Directory.Exists(folder))
                {
                    // Get all *.csv files
                    string[] filesCsv = Directory.GetFiles(folder, "product*.csv");

                    // Get all *.xlsx files
                    string[] filesXlsx = Directory.GetFiles(folder, "product*.xls");

                    // Combine both
                    string[] files = filesCsv.Concat(filesXlsx).ToArray();

                    if (files.Length > 0)
                    {
                        fileFound = true;
                        // Optionally, you can capture first matching file name here
                        // You can use: Dts.Variables["User::FileName"].Value = files[0];
