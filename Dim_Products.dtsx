                  <ProjectItem
                    Name="ScriptMain.cs"
                    Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using System.Net;
using System.Net.Mail;
using Microsoft.SqlServer.Dts.Runtime;
#endregion

namespace ST_9b78475dbd964d6cbf82f85c51fa3c13
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {
        public void Main()
        {
            try
            {
                

                string smtpServer = Dts.Variables["$Project::SmtpServer"].Value.ToString();
                int smtpPort = Convert.ToInt32(Dts.Variables["$Project::SmtpPort"].Value);
                string smtpUser = Dts.Variables["$Project::SmtpUsername"].Value.ToString();
                string smtpPass = Dts.Variables["$Project::SmtpPassword"].Value.ToString();
                string fromEmail = Dts.Variables["$Project::FromEmail"].Value.ToString();
                string toEmail = Dts.Variables["$Project::ToEmail"].Value.ToString();
                string subject = Dts.Variables["User::EmailSubjectFailure"].Value.ToString();
                string body = Dts.Variables["User::EmailBodyFailure"].Value.ToString();
                

                // Set up MailMessage
                MailMessage mail = new MailMessage();
                mail.From = new MailAddress(fromEmail);

                // Support multiple "To" addresses separated by semicolon
                foreach (string address in toEmail.Split(new char[] { ';' }, StringSplitOptions.RemoveEmptyEntries))
                {
                    mail.To.Add(address.Trim());
                }

                mail.Subject = subject;
                mail.Body = body;

                SmtpClient client = new SmtpClient(smtpServer, smtpPort)
                {
                    EnableSsl = true,
                    Credentials = new NetworkCredential(smtpUser, smtpPass)
                };

                client.Send(mail);
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Send Success Email", ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
    }
}]]></ProjectItem>
