            // remove line breaks / invalid chars for email subject
            //string safeError = errorMessage.Replace("\r", " ").Replace("\n", " ").Trim();

            //Dts.Variables["User::ErrorMessage"].Value = safeError;

            string errorMessage = Dts.Variables["System::ErrorDescription"].Value.ToString();
            string current = Dts.Variables["User::ErrorMessage"].Value.ToString();

            // Only save the first error
            if (string.IsNullOrEmpty(current))
            {
                // sanitize for email/log
                string safeError = errorMessage.Replace("\r", " ").Replace("\n", " ").Trim();
                Dts.Variables["User::ErrorMessage"].Value = safeError;
            }



