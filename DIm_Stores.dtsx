                bool fileFound = false;
                // Check if any files matching the pattern exist in the folder
                if (Directory.Exists(folder))
                {
                    // Get all *.csv files
                    string[] filesCsv = Directory.GetFiles(folder, "store*.csv");

                    // Get all *.xlsx files
                    string[] filesXlsx = Directory.GetFiles(folder, "store*.xlsx");

                    // Combine both
